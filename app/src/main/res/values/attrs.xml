<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="playerview">
        <attr name="cover" format="integer" />
        <attr name="buttonColor" format="color" />
        <attr name="progressEmptyColor" format="color" />
        <attr name="progressLoadedColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="textSize" format="dimension" />
    </declare-styleable>
    <declare-styleable name="TintImageView">
        <attr name="tint" format="reference|color" />
    </declare-styleable>
    <declare-styleable name="StateLayout">
        <attr name="errorDrawable" format="reference" />
        <attr name="emptyDrawable" format="reference" />
        <attr name="progressView" format="reference" />
    </declare-styleable>
</resources>